{
  "name": "self-approval",
  "version": "1.0.0",
  "private": false,
  "description": "A GitHub App built with Probot and that allows Pull Request authors to self-approve their Pull Requests.",
  "author": "iXOR Technology & Cubik65536",
  "repository": "https://github.com/self-approval/app",
  "license": "ISC",
  "homepage": "https://github.com/self-approval/app",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./build/app.js",
    "lint": "prettier --check '{src,test}/**/*.{js,json}' '*.{md,json,js}' 'contributing/*.md' '.github/**/*.{yml,md}'",
    "lint:fix": "prettier --write '{src,test}/**/*.{js,json}' '*.{md,json,js}' 'contributing/*.md' '.github/**/*.{yml,md}'",
    "test": "jest --coverage"
  },
  "dependencies": {
    "probot": "^12.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "codecov": "^3.8.3",
    "got": ">=14.0.0",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^3.1.0",
    "smee-client": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vercel": "^33.0.1"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
